OPENAI_API_KEY=sk-proj-...

BROWSER_URL=ws://127.0.0.1:5001/
# BENCHMARK_URL is loaded inside the remote browser (page.goto), so use a host-reachable
# address from Docker (see docker-compose.yml extra_hosts).
BENCHMARK_URL=http://host.docker.internal:3334
# Optional: used by host-side HTTP checks (urllib/curl), defaults to BENCHMARK_URL.
BENCHMARK_HTTP_URL=http://127.0.0.1:3334
